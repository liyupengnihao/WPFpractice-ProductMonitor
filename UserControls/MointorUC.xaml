<UserControl x:Class="ProductMonitor.UserControls.MointorUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ProductMonitor.UserControls"
             xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
             mc:Ignorable="d" 
             xmlns:rader="clr-namespace:ProductMonitor.UserControls"
             d:DesignHeight="675" d:DesignWidth="1200" Height="675" Width="1200">
    <!--lvc放容器控件内不会出错 折线图与饼状图-->
    <UserControl.Resources>
        <!--再此类的资源-->
        <Style TargetType="GroupBox">
            <Setter Property="Margin" Value="10,3">

            </Setter>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="GroupBox">
                        <Grid>
                            <!--左上角线条-->
                            <Polyline Points="0 30 0 10 10 0 30 0" Stroke="#9918aabd" StrokeThickness="1" VerticalAlignment="Top" HorizontalAlignment="Left"></Polyline>
                            <!--两圆点-->
                            <Ellipse Width="3" Height="4" HorizontalAlignment="Left" VerticalAlignment="Top" Fill="#9918aabd" Margin="24,-2,0,0"></Ellipse>
                            <Ellipse Width="3" Height="4" HorizontalAlignment="Left" VerticalAlignment="Top" Fill="#9918aabd" Margin="-2,24,0,0"></Ellipse>
                            <!--右线条路径-->
                            <Path Data="M0 0,3 3,30 3,33 0,63 0,83 20,83 40M5 0,28 0" HorizontalAlignment="Right" Stroke="#9918aabd" StrokeThickness="1"></Path>
                            <!--M代表移动-->
                            <!--左下角的线-->
                            <Polyline Points="0 0,0 15,10 25,25 25" Stroke="#9918aabd" StrokeThickness="1" VerticalAlignment="Bottom" HorizontalAlignment="Left"></Polyline>
                            <!--右下角线-->
                            <Polyline Points="10 0,0 10" Stroke="#9918aabd" StrokeThickness="1" VerticalAlignment="Bottom" HorizontalAlignment="Right"></Polyline>
                            <!--右下角多边形-->
                            <Polygon Points="0 7,7 7,7 0" Fill="#9918aabd" HorizontalAlignment="Right" VerticalAlignment="Bottom"></Polygon>
                            <!--上面的线,来四个-->
                            <Border BorderThickness="0,1,0,0"  BorderBrush="#9918aabd" Margin="20,-0.5,83,0"></Border>
                            <Border BorderThickness="1,0,0,0"  BorderBrush="#9918aabd" Margin="-0.5,20,0,20"></Border>
                            <Border BorderThickness="0,0,1,0"  BorderBrush="#9918aabd" Margin="-1,20,0,10"></Border>
                            <Border BorderThickness="0,0,0,1"  BorderBrush="#9918aabd" Margin="25,0,10,0"></Border>
                            <!--文字修饰-->
                            <Path Data="M0 0,3 0,5 4,3 8,0 8,3 4" Fill="#9918aabd" Margin="10,13"></Path>
                            <Path Data="M0 0,3 0,5 4,3 8,0 8,3 4" Fill="#5518aabd" Margin="16,13"></Path>
                            <TextBlock Text="{TemplateBinding Header}" Foreground="White" FontWeight="Bold" Margin="25,8" HorizontalAlignment="Left" VerticalAlignment="Top"></TextBlock>
                            <!--显示内容-->
                            <ContentPresenter></ContentPresenter>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </UserControl.Resources>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"></RowDefinition>
            <RowDefinition Height="1*"></RowDefinition>
            <RowDefinition Height="100"></RowDefinition>
        </Grid.RowDefinitions>
        <!--第一行-->
        <Grid Grid.Row="0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="200"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <!--第一行第一列-->
            <StackPanel Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                <TextBlock Text="{Binding TimerStr}" Foreground="White" FontSize="30" VerticalAlignment="Center" FontFamily="../Resource/Fonts/#iconfont"></TextBlock>
                <StackPanel>
                    <TextBlock Text="{Binding DateStr}" Foreground="White" FontSize="10" Margin="1,1,1,1"></TextBlock>
                    <TextBlock Text="{Binding WeekStr}" Foreground="White" FontSize="10" Margin="1,1,1,1" HorizontalAlignment="Right"></TextBlock>
                </StackPanel>
            </StackPanel>
            <!--第一行第二列-->
            <StackPanel Grid.Column="1" Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Left">
                <StackPanel.Resources>
                    <DataTemplate x:Key="machineCount">
                        <Border Width="15" Background="#3318aabd" Margin="2,0">
                            <TextBlock Text="{Binding}" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White" FontSize="16"></TextBlock>
                        </Border>
                    </DataTemplate>
                </StackPanel.Resources>
                <TextBlock Text="机台&#13;总数" Foreground="#99ffff" FontSize="10" VerticalAlignment="Center" Margin="5,0"></TextBlock>
                <ItemsControl ItemsSource="{Binding MachineCount}" ItemTemplate="{StaticResource machineCount}">
                    <ItemsControl.ItemsPanel>
                        <ItemsPanelTemplate>
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center"></StackPanel>
                        </ItemsPanelTemplate>
                    </ItemsControl.ItemsPanel>
                </ItemsControl>

                <TextBlock Text="生产&#13;计数" Foreground="#99ffff" FontSize="10" VerticalAlignment="Center" Margin="10,0"></TextBlock>
                <ItemsControl ItemsSource="{Binding ProductCount}" ItemTemplate="{StaticResource machineCount}">
                    <ItemsControl.ItemsPanel>
                        <ItemsPanelTemplate>
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center"></StackPanel>
                        </ItemsPanelTemplate>
                    </ItemsControl.ItemsPanel>
                </ItemsControl>

                <TextBlock Text="不良&#13;计数" Foreground="#99ffff" FontSize="10" VerticalAlignment="Center" Margin="10,0"></TextBlock>
                <ItemsControl ItemsSource="{Binding BadCount}" ItemTemplate="{StaticResource machineCount}">
                    <ItemsControl.ItemsPanel>
                        <ItemsPanelTemplate>
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center"></StackPanel>
                        </ItemsPanelTemplate>
                    </ItemsControl.ItemsPanel>
                </ItemsControl>
            </StackPanel>
            <!--第一行第三列 配置按钮 触发器 渐进色-->
            <Button Style="{StaticResource BtnStyle}" Grid.Column="2" HorizontalAlignment="Right" Height="35" Width="80" VerticalAlignment="Center" Margin="5,0"
                    Content="配置" Tag="&#xe61b;"></Button>
        </Grid>
        <!--第二行-->
        <Grid Grid.Row="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <!--第二行第一列-->
            <UniformGrid Grid.Column="0" Columns="1">
                <GroupBox Header="环境" Foreground="White">
                    <!--遍历所有实体-->
                    <ItemsControl ItemsSource="{Binding EnviromentList}" VerticalAlignment="Center">
                        <ItemsControl.ItemsPanel>
                            <ItemsPanelTemplate>
                                <UniformGrid>
                                    <UniformGrid.Columns>
                                        4
                                    </UniformGrid.Columns>
                                </UniformGrid>
                            </ItemsPanelTemplate>
                        </ItemsControl.ItemsPanel>
                        <ItemsControl.ItemTemplate>
                            <DataTemplate>
                                <!--用的多-->
                                <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,5">
                                    <TextBlock Text="{Binding EnItemValue}" FontSize="16" Foreground="#ff2bedf1" Margin="0,5"></TextBlock>
                                    <TextBlock Text="{Binding EnItemName}" FontSize="10" Foreground="white"></TextBlock>
                                </StackPanel>
                            </DataTemplate>
                        </ItemsControl.ItemTemplate>
                    </ItemsControl>
                </GroupBox>
                <GroupBox Header="产能" Foreground="White">
                    <Grid>
                        <!--定义x轴数据-->
                        <lvc:CartesianChart Margin="20,35,20,5">
                            <lvc:CartesianChart.AxisX>
                                <lvc:Axis Labels="8:00,9:00,10:00,11:00,12:00,13:00,14:00,15:00,16:00" Foreground="White" FontSize="10">
                                    <lvc:Axis.Separator>
                                        <lvc:Separator Step="1" StrokeThickness="2"></lvc:Separator>
                                    </lvc:Axis.Separator>
                                </lvc:Axis>
                            </lvc:CartesianChart.AxisX>
                            <!--定义Y轴数据-->
                            <lvc:CartesianChart.Series>
                                <lvc:ColumnSeries Values="300,400,500,380,200,600,350,550,610" Title="生产计数" MaxColumnWidth="10">
                                    <!--渐变色-->
                                    <lvc:ColumnSeries.Fill>
                                        <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                                            <GradientStop Color="#ff3fbbe6" Offset="0.0" />
                                            <GradientStop Color="#ff2bedf1" Offset="1.0" />
                                        </LinearGradientBrush>
                                    </lvc:ColumnSeries.Fill>
                                </lvc:ColumnSeries>

                                <lvc:ColumnSeries Values="100,0,80,100,0,0,150,100,0" Title="不良计数" MaxColumnWidth="10">
                                    <!--渐变色-->
                                    <lvc:ColumnSeries.Fill>
                                        <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                                            <GradientStop Color="#fffb9a9a" Offset="0.0" />
                                            <GradientStop Color="#ffff5151" Offset="1.0" />
                                        </LinearGradientBrush>
                                    </lvc:ColumnSeries.Fill>
                                </lvc:ColumnSeries>

                            </lvc:CartesianChart.Series>
                            <!--Y数据刻度-->
                            <lvc:CartesianChart.AxisY>
                                <lvc:Axis MinValue="0" MaxValue="500">
                                    <lvc:Axis.Separator>
                                        <lvc:Separator Step="100" Stroke="#11ffff"></lvc:Separator>
                                    </lvc:Axis.Separator>
                                </lvc:Axis>
                            </lvc:CartesianChart.AxisY>
                        </lvc:CartesianChart>
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="10">
                            <Border Height="6" Width="6" Background="#ff2bedf1" Margin="5,0"></Border>
                            <TextBlock Text="生产计数" FontSize="10" Foreground="White" Margin="5,0"></TextBlock>
                            <Border Height="6" Width="6" Background="#ffff5151" Margin="5,0"></Border>
                            <TextBlock Text="不良计数" FontSize="10" Foreground="White" Margin="5,0"></TextBlock>
                        </StackPanel>
                    </Grid>
                </GroupBox>
                <GroupBox Header="质量" Foreground="White">

                    <lvc:CartesianChart Margin="20,35,20,5">
                        <!--X轴数据-->
                        <lvc:CartesianChart.AxisX>
                            <lvc:Axis Labels="1#,2#,3#,4#,5#,6#" Foreground="White" FontSize="10">
                                <lvc:Axis.Separator>
                                    <lvc:Separator Step="1" StrokeThickness="2"></lvc:Separator>
                                </lvc:Axis.Separator>
                            </lvc:Axis>
                        </lvc:CartesianChart.AxisX>
                        <!--Y轴数据-->
                        <lvc:CartesianChart.Series>
                            <lvc:LineSeries Values="8,2,7,6,4,14" PointGeometrySize="0" Stroke="#ff2bedf1" Title="生产计数">
                                <!--渐变色-->
                                <lvc:LineSeries.Fill>
                                    <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                                        <GradientStop Color="#552bedf1" Offset="0.0" />
                                        <GradientStop Color="Transparent" Offset="1.0" />
                                    </LinearGradientBrush>
                                </lvc:LineSeries.Fill>
                            </lvc:LineSeries>
                        </lvc:CartesianChart.Series>
                        <!--Y数据刻度-->
                        <lvc:CartesianChart.AxisY>
                            <lvc:Axis MinValue="0" MaxValue="15">
                                <lvc:Axis.Separator>
                                    <lvc:Separator Step="5" Stroke="#11ffff"></lvc:Separator>
                                </lvc:Axis.Separator>
                            </lvc:Axis>
                        </lvc:CartesianChart.AxisY>
                    </lvc:CartesianChart>
                </GroupBox>
            </UniformGrid>
            <!--第二行第二列-->
            <Grid Grid.Column="1">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="auto"/>
                    <RowDefinition Height="1.5*"/>
                </Grid.RowDefinitions>
                <!--第一行-->
                <Image Grid.Row="0" Source="../Resource/Images/牛马.jpg" Height="160" VerticalAlignment="Center"/>
                <!--第二行-->
                <Grid Grid.Row="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center">
                        <TextBlock Text="&#xe658;" FontFamily="../Resource/Fonts/#iconfont" Foreground="#99ffff" Margin="5,0"></TextBlock>
                        <TextBlock Text="报警记录" Foreground="white" Margin="5,0"></TextBlock>
                    </StackPanel>
                    <!--第二行 报警列表-->
                    <ItemsControl Grid.Row="1" ItemsSource="{Binding AlarmList}" Margin="5,0">
                        <ItemsControl.ItemTemplate>
                            <DataTemplate>
                                <Grid Height="23">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="20"/>
                                        <ColumnDefinition Width="30"/>
                                        <ColumnDefinition Width="150"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <Border Background="IndianRed" Height="6" Width="6" CornerRadius="3" HorizontalAlignment="Center" VerticalAlignment="Center"></Border>
                                    <TextBlock Text="{Binding Num}" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#992bedf1"></TextBlock>
                                    <TextBlock Text="{Binding Msg}" Grid.Column="2" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#992bedf1"></TextBlock>
                                    <TextBlock Text="{Binding Time}" Grid.Column="3" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#992bedf1"></TextBlock>
                                    <TextBlock Text="{Binding Duration,StringFormat=时长{0}秒}" Grid.Column="4" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#992bedf1"></TextBlock>
                                </Grid>

                            </DataTemplate>
                        </ItemsControl.ItemTemplate>
                    </ItemsControl>

                </Grid>
                <!--第三行-->
                <GroupBox Grid.Row="2" Header="数据异常报警比例" Margin="0,0,0,5" Height="150" VerticalAlignment="Bottom">
                    <!--饼形图-->
                    <!--定义数据标签样式-->
                    <lvc:PieChart InnerRadius="25" Margin="0,40,0,20">
                        <lvc:PieChart.Resources>
                            <Style TargetType="lvc:PieSeries">
                                <Setter Property="DataLabelsTemplate">
                                    <Setter.Value>
                                        <DataTemplate>
                                            <StackPanel Orientation="Horizontal">
                                                <TextBlock Text="{Binding Point.SeriesView.Title}" Margin="0,0,5,0" Foreground="#44ffffff"></TextBlock>
                                                <TextBlock Text="{Binding Point.SeriesView.Values[0]}" Foreground="#44ffffff"></TextBlock>
                                            </StackPanel>
                                        </DataTemplate>
                                    </Setter.Value>
                                </Setter>
                            </Style>
                        </lvc:PieChart.Resources>
                        <!--定义饼形图数据-->
                        <lvc:PieChart.Series>
                            <lvc:PieSeries Values="20" StrokeThickness="0" Title="压差" DataLabels="true" LabelPosition="OutsideSlice"></lvc:PieSeries>
                            <lvc:PieSeries Values="40" StrokeThickness="0" Title="振动" DataLabels="true" LabelPosition="OutsideSlice"></lvc:PieSeries>
                            <lvc:PieSeries Values="10" StrokeThickness="0" Title="设备温度" DataLabels="true" LabelPosition="OutsideSlice"></lvc:PieSeries>
                            <lvc:PieSeries Values="40" StrokeThickness="0" Title="光照" DataLabels="true" LabelPosition="OutsideSlice"></lvc:PieSeries>
                        </lvc:PieChart.Series>
                    </lvc:PieChart>

                </GroupBox>
            </Grid>
            <!--第二行第三列-->
            <UniformGrid Grid.Column="2" Columns="1">
                <!--第一行-->
                <GroupBox Header="设备">
                    <!--遍历所有实体-->
                    <ItemsControl ItemsSource="{Binding DeviceList}" VerticalAlignment="Center">
                        <ItemsControl.ItemsPanel>
                            <ItemsPanelTemplate>
                                <UniformGrid>
                                    <UniformGrid.Columns>
                                        4
                                    </UniformGrid.Columns>
                                </UniformGrid>
                            </ItemsPanelTemplate>
                        </ItemsControl.ItemsPanel>
                        <ItemsControl.ItemTemplate>
                            <DataTemplate>
                                <!--用的多-->
                                <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,5">
                                    <TextBlock Text="{Binding Value}" FontSize="16" Foreground="#ff2bedf1" Margin="0,5"></TextBlock>
                                    <TextBlock Text="{Binding DeviceItem}" FontSize="10" Foreground="white"></TextBlock>
                                </StackPanel>
                            </DataTemplate>
                        </ItemsControl.ItemTemplate>
                    </ItemsControl>
                </GroupBox>
                <GroupBox Header="能耗">
                    <!--<rader:ReaderUC></rader:ReaderUC>-->
                </GroupBox>
                <GroupBox Header="人力">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <StackPanel Grid.Column="0" VerticalAlignment="Center" Margin="0,20,0,0">
                            <TextBlock Text="870" HorizontalAlignment="Center" Foreground="#99ffffff" FontSize="30"></TextBlock>
                            <TextBlock Text="在职在岗" HorizontalAlignment="Center" Foreground="#99ffffff" FontSize="15"></TextBlock>
                        </StackPanel>

                        <Grid Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="30"></RowDefinition>
                                <RowDefinition></RowDefinition>
                            </Grid.RowDefinitions>
                            <TextBlock Grid.Row="0" Text="缺岗统计" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="#55ffffff" FontSize="13"></TextBlock>
                            <ItemsControl Grid.Row="1" ItemsSource="{Binding StuffOutWorkList}">
                                <ItemsControl.ItemTemplate>
                                    <DataTemplate>
                                        <Grid Height="20" Width="auto">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="10"></ColumnDefinition>
                                                <ColumnDefinition Width="50"></ColumnDefinition>
                                                <ColumnDefinition Width="40"></ColumnDefinition>
                                                <ColumnDefinition Width="*"></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <Border Width="5" Height="5" CornerRadius="5" Background="Orange" Margin="0,0,10,0"></Border>
                                            <TextBlock Text="{Binding StuffName}" Grid.Column="1" Foreground="#99ffffff"></TextBlock>
                                            <TextBlock Text="{Binding Position}" Grid.Column="2" Foreground="#99ffffff"></TextBlock>
                                            <StackPanel Grid.Column="3" Orientation="Horizontal">
                                                <Line Stroke="#aa2bedf1" X1="0" Y1="0" X2="{Binding ShowWidt}" Y2="0" StrokeThickness="3" Margin="5,0"></Line>
                                                <TextBlock Text="{Binding OUtWorkCount}" FontSize="9" Foreground="#99ffffff"></TextBlock>
                                            </StackPanel>
                                        </Grid>
                                    </DataTemplate>
                                </ItemsControl.ItemTemplate>
                            </ItemsControl>
                        </Grid>
                    </Grid>
                </GroupBox>
            </UniformGrid>
        </Grid>
        <!--第三行-->
        <ItemsControl Grid.Row="3" ItemsSource="{Binding WorkShopList}">
            <ItemsControl.ItemsPanel>
                <ItemsPanelTemplate>
                    <UniformGrid Rows="1"></UniformGrid>
                </ItemsPanelTemplate>
            </ItemsControl.ItemsPanel>
            <ItemsControl.ItemTemplate>
                <DataTemplate>
                    <Border BorderThickness="0,1,0,0" BorderBrush="#9918aabd" Background="#01a8aabd" Margin="10,5">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto"></RowDefinition>
                                <RowDefinition></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="0" Text="{Binding WorkShopName}" Foreground="#18aabd"></TextBlock>
                                <!--<TextBlock Grid.Column="2" Text="[详情]" Foreground="White"></TextBlock>-->
                                <TextBlock Grid.Column="2">
                                    <Hyperlink Foreground="white" TextDecorations="None" Command="{Binding ShowDetailCmm,RelativeSource={RelativeSource AncestorType=Window}}">
                                        [详情]
                                    </Hyperlink><!--超链接-->
                                </TextBlock>
                            </Grid>
                            <Grid Grid.Row="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <StackPanel Orientation="Vertical" Grid.Column="0">
                                    <TextBlock Text="机台总数" FontSize="10" Foreground="#99ffffff"></TextBlock>
                                    <TextBlock Text="{Binding TotalCount}" FontSize="20" Foreground="white" Margin="0,10,0,0"></TextBlock>
                                </StackPanel>
                                <StackPanel Orientation="Vertical" Grid.Column="1">
                                    <TextBlock Text="{Binding WorkingCount}" Foreground="LightSeaGreen"></TextBlock>
                                    <TextBlock Text="作业" Margin="0,0,0,5" Foreground="#55ffffff"></TextBlock>
                                    <TextBlock Text="{Binding WrongCount}" Foreground="Red"></TextBlock>
                                    <TextBlock Text="故障" Foreground="#55ffffff"></TextBlock>
                                </StackPanel>
                                <StackPanel Orientation="Vertical" Grid.Column="2">
                                    <TextBlock Text="{Binding WaitCount}" Foreground="Orange"></TextBlock>
                                    <TextBlock Text="等待" Margin="0,0,0,5" Foreground="#55ffffff"></TextBlock>
                                    <TextBlock Text="{Binding StopCount}" Foreground="#55ffffff"></TextBlock>
                                    <TextBlock Text="停机" Foreground="#55ffffff"></TextBlock>
                                </StackPanel>
                            </Grid>
                        </Grid>
                    </Border>
                </DataTemplate>
            </ItemsControl.ItemTemplate>
        </ItemsControl>
    </Grid>
</UserControl>
